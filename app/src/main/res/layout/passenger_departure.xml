                    android:layout_height="wrap_content"
                    android:textColor="#fff"
                    android:checked="false"
                    android:buttonTint="#fff"

                    android:id="@+id/rd_wal_chair"

                />
                <CheckBox
                    android:layout_marginStart="@dimen/_50sdp"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#fff"
                    android:checked="false"
                    android:buttonTint="#fff"
